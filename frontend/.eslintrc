{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "linebreak-style": ["error", "windows"],
    "react/jsx-one-expression-per-line": "off"

  },
  "globals": {
    "fetch": false,
    "document": true
  }
}
